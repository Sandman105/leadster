  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "knex": "^0.20.3",
    "mongoose": "^5.7.12",
    "mysql": "^2.17.1",
    "mysql2": "^2.0.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.1"
  }
}
